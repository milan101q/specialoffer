            href.includes('/vehicle/') || 
            href.includes('/inventory/') || 
            href.includes('/car/') || 
            href.includes('/detail/') || 
            href.includes('/used-') ||
            href.includes('/listing/') ||
            href.includes('/vdp/')) ||
