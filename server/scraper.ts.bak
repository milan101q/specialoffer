          }
          
          if (imgSrc && !imgSrc.includes('placeholder') && !imgSrc.includes('blank.gif') && !imgSrc.includes('loading')) {
            try {
              const absoluteImgUrl = new URL(imgSrc, url).toString();
              if (!images.includes(absoluteImgUrl) && 
